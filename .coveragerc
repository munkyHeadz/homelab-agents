[run]
source = crews/approval
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
# Only fail on approval module coverage
fail_under = 20
skip_covered = True
skip_empty = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
